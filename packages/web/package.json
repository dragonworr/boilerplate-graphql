{
  "private": true,
  "name": "@fullstack-boilerplate/web",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "serve": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "DEBUG=true graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "lint": "eslint --fix --cache --ext .ts,.tsx src",
    "lint:test": "eslint --cache --ext .ts,.tsx src",
    "prettify": "prettier --write src",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|jsx|json|ts|tsx|html)|!graphql.tsx": [
      "prettier --write"
    ],
    "*.(ts|tsx)|!graphql.tsx": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.3.19",
    "@chakra-ui/react": "1.6.2",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@hookform/resolvers": "2.5.1",
    "@sentry/react": "6.4.1",
    "@sentry/tracing": "6.4.1",
    "components": "link:./src/components",
    "dayjs": "1.10.4",
    "framer-motion": "4.1.17",
    "lib": "link:./src/lib",
    "next": "10.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.6.6",
    "react-icons": "4.2.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@types/cookie": "0.4.0",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0"
  }
}
